{
  "name": "@projects-md/markdown-entity-parser",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Daniel Kahl <me@danto.dev>",
  "description": "Parse entities from markdown files into JSON/YAML via CLI or NodeJS API",
  "keywords": [],
  "type": "module",
  "main": "src/main.js",
  "bin": {
    "parse-md": "src/cli.js"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "parse-md": "src/cli.js",
    "lint": "eslint --max-warnings 0 --ext .js,.json .",
    "lint:fix": "eslint --max-warnings 0 --ext .js,.json . --fix",
    "test": "jest",
    "test:cov": "jest --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "glob": "^10.3.3",
    "lodash-es": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "unist-util-visit": "^5.0.0",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "babel-jest": "^29.5.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "lint-staged": "^13.2.3",
    "prettier": "^2.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "npm run lint:fix"
  }
}
